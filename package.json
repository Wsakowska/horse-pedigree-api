{
  "name": "horse-pedigree-api",
  "version": "1.0.0",
  "description": "REST API and frontend for horse pedigree database",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "migrate": "npx knex migrate:latest",
    "seed": "npx knex seed:run",
    "test": "jest --coverage",
    "test:backend": "jest --testPathPattern=backend",
    "test:frontend": "jest --testPathPattern=frontend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "knex": "^2.5.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "uuid": "^9.0.1",
    "whatwg-encoding": "^3.1.1"
  }
}